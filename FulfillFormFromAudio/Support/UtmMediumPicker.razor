    <option value="@(UtmMedium.CORE_PKL_SEA)">SEA</option>
    <option value="@(UtmMedium.CORE_PKL_Perfmax)">Perf Max</option>
    <option value="@(UtmMedium.CORE_PKL_Video)">Vidéo</option>
    <option value="@(UtmMedium.STUDI_PKL_Import)">Import</option>
    <option value="@(UtmMedium.STUDI_PKL_Whatsapp)">Whatsapp</option>
</InputSelect>
@code {
    [Parameter]
    public UtmMedium Value { get; set; }
    [Parameter]
    public EventCallback<UtmMedium> ValueChanged { get; set; }
    [Parameter]
    public Expression<Func<UtmMedium>>? ValueExpression { get; set; }
}
