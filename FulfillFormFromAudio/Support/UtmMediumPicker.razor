    <option value="@(UtmMedium.CORE_PKL_PT)">Partenariat (PT)</option>
    <option value="@(UtmMedium.CORE_PKL_Referral)">Référence</option>
    <option value="@(UtmMedium.CORE_PKL_Site)">Sites Gge</option>
    <option value="@(UtmMedium.CORE_PKL_Smo)">SMO</option>
    <option value="@(UtmMedium.CORE_PKL_Sms)">SMS</option>
    <option value="@(UtmMedium.CORE_PKL_None)">Aucun</option>
    <option value="@(UtmMedium.CORE_PKL_Affiliate)">Affilié</option>
    <option value="@(UtmMedium.CORE_PKL_Print)">Imprimer</option>
