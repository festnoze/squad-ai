{
  "ConnectionStrings": {
    "LMSConnection": "Data Source=localhost;initial catalog=STUDILMS_LOCAL;persist security info=True;user id=sqldev;password=sqldev;multipleactiveresultsets=True;application name=LMS_Local"
  },
  //"DatabaseKind": "NEW_DB",
  "DatabaseKind": "LMS_DB",
  "DatabaseEnvironment": "LOCAL_STUDI",
  "UnifiedApiUri": "https://api.studi.fr.dev",
  "CommandTimeout": "00:05:00",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Studi.Api.Lms.Messenger.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Debug"
      }
    ]
  },
  "Swagger": {
    "IsEnabled": true,
    "SubPath": "lms/messenger",
    "UseBearerConfiguration": true,
    "UseApiVersioning": true,
    "UseSwaggerExamples": false
