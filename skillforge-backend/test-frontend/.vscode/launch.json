{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Streamlit",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/startup.py",
        "--server.port",
        "8577"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
